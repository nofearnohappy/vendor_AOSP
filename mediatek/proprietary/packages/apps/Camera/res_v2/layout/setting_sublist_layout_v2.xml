<?xml version="1.0" encoding="utf-8"?>
<com.android.camera.v2.ui.SettingSublistLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:camera="http://schemas.android.com/apk/res/com.mediatek.camera"
    style="@style/SettingContainer2Parent"
    android:splitMotionEvents="false">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:splitMotionEvents="false">
        <com.android.camera.v2.ui.MaxLinearLayout
            android:id="@+id/container"
            style="@style/SettingContainer2"
            camera:maxHeight="@dimen/setting_container2_height_max"
            android:splitMotionEvents="false">
            <ListView android:id="@+id/settingList"
                style="@style/SettingItemList" 
                android:splitMotionEvents="false"/>
        </com.android.camera.v2.ui.MaxLinearLayout>
    </RelativeLayout>
</com.android.camera.v2.ui.SettingSublistLayout>
