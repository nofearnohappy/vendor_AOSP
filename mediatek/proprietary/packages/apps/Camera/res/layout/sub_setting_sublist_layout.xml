<?xml version="1.0" encoding="utf-8"?>
<com.android.camera.ui.SettingSublistLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:camera="http://schemas.android.com/apk/res/com.mediatek.camera"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:layout_gravity="center">
	<com.android.camera.ui.MaxLinearLayout
		android:id="@+id/container" style="@style/SettingNewContainer2"
		camera:maxHeight="@dimen/setting_new_container2_height_max">
		<TextView android:id="@+id/ItemListTitle" style="@style/ItemListTitle" />
		<LinearLayout android:layout_width="match_parent"
			android:layout_height="match_parent" android:background="@drawable/sub_setting_seperator" />
		<ListView android:id="@+id/settingList" style="@style/SettingItemList" />
	</com.android.camera.ui.MaxLinearLayout>
</com.android.camera.ui.SettingSublistLayout>