<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item
        android:state_enabled="true"
        android:state_pressed="true"
        android:drawable="@drawable/camera_slow_motion_shutter_pressed" />
  <item
        android:state_enabled="true"
        android:state_focused="true"
        android:drawable="@drawable/camera_slow_motion_shutter_focus" />
  <item
        android:state_enabled="true"
        android:drawable="@drawable/camera_slow_motion_shutter_stop" /> 
  <item
        android:state_enabled="false"
        android:state_focused="true"
        android:drawable="@drawable/camera_slow_motion_shutter_disable_focus" />
  <item
        android:state_enabled="false"
        android:drawable="@drawable/camera_slow_motion_shutter_disable"/>
</selector>
